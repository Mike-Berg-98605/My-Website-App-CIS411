@{
    ViewData["Title"] = "Contact Page";
}

<div class="text-center">
    <h1 class="display-4">Contact Me</h1>
    <table class="table">
        <tbody>
            <tr>
                <td>Phone:</td><td>@ViewBag.Phone</td>
            </tr>
            <tr>
                <td>Email:</td><td>@ViewBag.Email</td>
            </tr>
            <tr>
                <td>Facebook:</td><td>@ViewBag.Facebook</td>
            </tr>
        </tbody>
    </table>
</div>
